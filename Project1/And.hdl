/**
 * And gate: 
 * out = 1 if (a == 1 and b == 1)
 *       0 otherwise
 */

CHIP And {
    IN a, b;
    OUT out;

    PARTS:
    // Simplified Boolean Expression: (A'*B')'
    Not(in=a, out=nota);
    Not(in=b, out=notb);
    Nor(a=nota, b=notb, out=out);
}
